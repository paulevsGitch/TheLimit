{
	"multipart": [
		{
			"apply": { "model": "thelimit:block/stellata_branch_center" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side", "x": 90, "y": 180 },
			"when": { "north": "true" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side", "x": 90 },
			"when": { "south": "true" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side", "x": 90, "y": 270 },
			"when": { "east": "true" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side", "x": 90, "y": 90 },
			"when": { "west": "true" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side", "x": 180 },
			"when": { "up": "true" }
		},
		{
			"apply": { "model": "thelimit:block/stellata_branch_side" },
			"when": { "down": "true" }
		}
	]
}
